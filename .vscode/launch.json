    "version": "0.2.0",
    "configurations": [
        {
            "name": "samples",
            "type": "debugpy",
            "request": "launch",
            "program": "./samples.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "poseidon",
            "type": "debugpy",
            "request": "launch",
            "program": "./scOT/train-sahba.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config",
                "./configs/run.yaml",
                "--data_path",
                "./data",
                "--checkpoint_path",
                "./ckpts",
                "--wandb_run_name",
                "poseidonB_ursell",
                "--finetune_from",
                "camlab-ethz/Poseidon-B",
                "--max_num_train_time_steps",
                "10",
                "--train_time_step_size",
                "2",
                "--replace_embedding_recovery"

            ]
        },
        {
            "name": "poseidon-infer-bubble",
            "type": "debugpy",
            "request": "launch",
            "program": "./scOT/inference-sahba.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--model_path",
                "./ckpts1/scOT/poseidonB_ursell",
                "--ckpt_dir",
                "./ckpts1",
                "--data_path",
                "./data",
                "--dataset",
                "fluids.compressible.Bubble",
                "--mode",
                "eval",
                "--append_time",
                "--batch_size",
                "16",
                "--initial_time",
                "5",
                "--final_time",
                "10",
            ]
        }
    ]
}
    "version": "0.2.0",
    "configurations": [
        {
            "name": "samples",
            "type": "debugpy",
            "request": "launch",
            "program": "./samples.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "poseidon",
            "type": "debugpy",
            "request": "launch",
            "program": "./scOT/train.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config",
                "./configs/run.yaml",
                "--data_path",
                "./data",
                "--checkpoint_path",
                "./ckpts",
                "--wandb_run_name",
                "poseidon-test-ursell",
            ]
        },
        {
            "name": "poseidon-infer-Riemann",
            "type": "debugpy",
            "request": "launch",
            "program": "./scOT/inference.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--model_path",
                "camlab-ethz/Poseidon-B",
                "--ckpt_dir",
                "./ckpts",
                "--data_path",
                "./data",
                "--dataset",
                "fluids.compressible.Riemann.time",
                "--mode",
                "eval",
                "--append_time",
            ]
        },
        {
            "name": "poseidon-infer-bubble",
            "type": "debugpy",
            "request": "launch",
            "program": "./scOT/inference.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--model_path",
                "./ckptsss/scOT/posiedon-test-ursell",
                "--ckpt_dir",
                "./ckptsss",
                "--data_path",
                "./data",
                "--dataset",
                "fluids.compressible.Bubble.time.out",
                "--mode",
                "eval",
                "--append_time",
                "--batch_size",
                "5",
                "--initial_time",
                "50",
                "--final_time",
                "55",
            ]
        }
    ]
}